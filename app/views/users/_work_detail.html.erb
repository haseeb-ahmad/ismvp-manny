<div class="education_detail">
  <h2>Work Detail</h2>
  <table class="table">
    <thead>
      <tr>
        <th>Employer</th>
        <th>Position</th>
        <th>Location</th>
        <th>Start Date</th>
        <th>End Date</th>
      </tr>
    </thead>
    <tbody>
      <% value.each do |val| %>
        <tr>
          <td> 
            <% if val["employer"].present? %>
              <%= val["employer"]["name"] %> </td>
            <% end %>
          <td> 
            <% if val["position"].present? %>
              <%= val["position"]["name"] %> </td>
            <% end %>
          <td>  
            <% if val["location"].present? %>
              <%= val["location"]["name"] %>
            <% end %>
          </td>
          <td> 
            <% if val["start_date"].present? %>
              <%= val["start_date"] %> </td>
            <% end %>
          <td> 
            <% if val["end_date"].present? %>
              <%= val["end_date"] %> </td>
            <% end %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>