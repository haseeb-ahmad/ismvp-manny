<div class="container">
		<h1>Listing Notes</h1>
	<table class="table">
		<thead>
			<tr>
				<th>Note</th>
				<th></th>
				<th>Created At</th>
				<th colspan="3"></th>
			</tr>
		</thead>

		<tbody>
			<% @notes.each do |note| %>
				<tr>
					<td><%= note.note %></td>
					<td></td>
					<td><%= note.updated_at.strftime("%B %e, %Y at %I%p") %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>
